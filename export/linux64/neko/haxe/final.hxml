-main ApplicationMain 
-cp /usr/lib/haxe/lib/flixel/3,3,6
-D flixel=3.3.6
-cp /usr/lib/haxe/lib/lime/2,0,6
-D lime=2.0.6
-cp /usr/lib/haxe/lib/openfl/2,2,4
-D openfl=2.2.4
-cp source
-cp /usr/lib/haxe/lib/lime/2,0,6/legacy
-D tools=2.0.6
